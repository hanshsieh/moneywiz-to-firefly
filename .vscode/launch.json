{
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Current File",
      "program": "${file}",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/build/**/*.js"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
    },
  ]
}
